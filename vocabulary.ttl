@base <https://wwkb.kiridaruma.net/resources/vocabulary.ttl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.

<Person>
    a rdfs:Class;
    rdfs:subClassOf schema:Person;
    rdfs:label "ワールドウィッチーズシリーズの登場人物"@ja.

<Witch>
    a rdfs:Class;
    rdfs:subClassOf <Person>;
    rdfs:label "ウィッチ"@ja.

<Country>
    a rdfs:Class;
    rdfs:subClassOf schema:Country;
    rdfs:label "ワールドウィッチーズシリーズの国家"@ja.
